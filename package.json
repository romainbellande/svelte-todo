		"clsx": "^2.1.1",
		"dotenv": "^16.4.7",
		"drizzle-kit": "^0.29.1",
		"eslint": "^9.7.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.46.1",
		"formsnap": "^2.0.0",
		"globals": "^15.0.0",
		"lucide-svelte": "^0.468.0",
		"prettier": "^3.3.2",
